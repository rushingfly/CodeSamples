{
    "manifest_version": 3,
    "name": "Cookie Copier",
    "version": "1.0.0",
    "permissions": [
      "cookies",
      "activeTab",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_title": "Copy Cookies"
    },
    "host_permissions": [
      "*://*/*"
    ]
  }